service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow write: if false;
      allow read: if true;
    }
    match /viewers/{key} {
      allow create, update: if true;
    }
    match /tests/{key} {
      allow write: if true;
      match /{document}/{key}{
        allow write: if true;
      }
    }
  }
}