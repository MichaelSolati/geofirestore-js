{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "outDir": "./dist",
    "moduleResolution": "node",
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "declaration": true,
    "removeComments": false,
    "target": "es2017",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"]
  },
  "include": ["src/**/*"]
}